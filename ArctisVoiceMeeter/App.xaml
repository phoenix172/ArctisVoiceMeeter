<Application x:Class="ArctisVoiceMeeter.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ArctisVoiceMeeter"
             xmlns:model="clr-namespace:ArctisVoiceMeeter.Model"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
             <DataTemplate DataType="{x:Type model:ChannelBindingViewModel}">
                 <local:ChannelBindingView />
             </DataTemplate>
         </ResourceDictionary>
    </Application.Resources>
</Application>
